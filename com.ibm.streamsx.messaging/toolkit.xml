<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<toolkitModel xmlns="http://www.ibm.com/xmlns/prod/streams/spl/toolkit" productVersion="3.2.1.0" xmlns:common="http://www.ibm.com/xmlns/prod/streams/spl/common" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <toolkit name="com.ibm.streamsx.messaging" requiredProductVersion="3.2.0" version="1.0.0">
    <description/>
    <uriTable>
      <uri index="2" value="com.ibm.streamx.messaging.mqtt/MqttSourceOperator"/>
      <uri index="1" value="com.ibm.streamx.messaging.mqtt/MqttSinkOperator"/>
    </uriTable>
    <namespace name="com.ibm.streamx.messaging.mqtt">
      <primitiveOp language="Java" modelUriIndex="1" name="MqttSinkOperator" public="true">
        <description>Java Operator MqttSinkOperator</description>
        <parameter expressionMode="Constant" name="vmArg" optional="true" type="rstring">
          <description>Command line options passed to the Java Virtual Machine at startup</description>
        </parameter>
        <parameter cardinality="1" expressionMode="AttributeFree" name="period" optional="true" type="float32">
          <description>Reconnection period, default is 60 s.</description>
        </parameter>
        <parameter cardinality="1" expressionMode="AttributeFree" name="qos" optional="false" type="int32">
          <description>Qos to publish to.</description>
        </parameter>
        <parameter cardinality="1" expressionMode="AttributeFree" name="reconnectionBound" optional="true" type="int32">
          <description>Reconnection bound, 0 for no retry, n for n number of retries, -1 for inifinite retry.</description>
        </parameter>
        <parameter cardinality="1" expressionMode="AttributeFree" name="serverURI" optional="false" type="rstring">
          <description>Server to publish messages to.</description>
        </parameter>
        <parameter cardinality="1" expressionMode="AttributeFree" name="topic" optional="false" type="rstring">
          <description>Topic to publish to.</description>
        </parameter>
        <inputPort maxNumPorts="1" minNumPorts="1" optional="false" windowPunctInputMode="Oblivious">
          <description>Port that ingests tuples</description>
          <windowPolicy>NonWindowed</windowPolicy>
        </inputPort>
        <inputPort minNumPorts="0" optional="true" windowPunctInputMode="Oblivious">
          <description>Optional input ports</description>
          <windowPolicy>NonWindowed</windowPolicy>
        </inputPort>
      </primitiveOp>
      <primitiveOp language="Java" modelUriIndex="2" name="MqttSourceOperator" public="true">
        <description>Java Operator MqttSourceOperator</description>
        <parameter expressionMode="Constant" name="vmArg" optional="true" type="rstring">
          <description>Command line options passed to the Java Virtual Machine at startup</description>
        </parameter>
        <parameter cardinality="-1" expressionMode="AttributeFree" name="qos" optional="false" type="int32">
          <description>List of qos for topic subscriptions</description>
        </parameter>
        <parameter cardinality="1" expressionMode="AttributeFree" name="serverURI" optional="false" type="rstring">
          <description>Server to subscribe messages from.</description>
        </parameter>
        <parameter cardinality="-1" expressionMode="AttributeFree" name="topics" optional="false" type="rstring">
          <description>List of topics to subscribe to.</description>
        </parameter>
        <outputPort expressionMode="Nonexistent" maxNumPorts="1" minNumPorts="1" optional="false" windowPunctOutputMode="Generating">
          <description>Port that produces tuples</description>
        </outputPort>
        <outputPort expressionMode="Nonexistent" minNumPorts="0" optional="true" windowPunctOutputMode="Generating">
          <description>Optional output ports</description>
        </outputPort>
      </primitiveOp>
    </namespace>
  </toolkit>

</toolkitModel>
